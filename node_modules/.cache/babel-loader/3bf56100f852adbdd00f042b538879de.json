) => {\n        state.error = 0\n        state.responseMsg = \"\"\n    },\n    setTest: (state, credentials) => {\n        state.test = credentials\n    }\n}\n\nexport default {\n    state,\n    getters,\n    actions,\n    mutations\n}"]}]}