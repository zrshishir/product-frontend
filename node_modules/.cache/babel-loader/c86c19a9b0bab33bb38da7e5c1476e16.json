A,GAAA,MAAA,CAAA,MAAA,CAAA,EAAA,EAAA,KAAA,CAAA,WAAA,CAAA;AACA,QAAA,KAAA,CAAA,WAAA,GAAA,CAAA,CAAA;AACA,OAHA;AAIA,KA3BA;AA6BA,IAAA,IA7BA,kBA6BA;AACA,UAAA,KAAA,WAAA,GAAA,CAAA,CAAA,EAAA;AACA,aAAA,MAAA,CAAA,QAAA,CAAA,OAAA,EAAA,CAAA,KAAA,MAAA,EAAA,KAAA,UAAA,CAAA;AACA,QAAA,MAAA,CAAA,MAAA,CAAA,KAAA,SAAA,CAAA,KAAA,CAAA,KAAA,WAAA,CAAA,EAAA,KAAA,UAAA;AACA,OAHA,MAGA;AACA,aAAA,MAAA,CAAA,QAAA,CAAA,OAAA,EAAA,CAAA,KAAA,MAAA,EAAA,KAAA,UAAA,CAAA;AACA,aAAA,SAAA,CAAA,KAAA,CAAA,IAAA,CAAA,KAAA,UAAA;AACA;;AACA,WAAA,KAAA;AACA;AAtCA;AA3DA,CAAA","sourcesContent":["<template>\n    <v-container v-if=\"isLoggedIn\">\n      <template>\n        <v-alert type=\"error\" v-if=\"error == 1\">\n            {{errorMsg}}\n          </v-alert>\n          <v-alert type=\"success\" v-if=\"error == 0\">\n            {{errorMsg}}\n          </v-alert>\n          <v-row justify=\"center\">\n            <v-dialog v-if=\"error\" persistent max-width=\"500\">\n              <v-card>\n                <v-card-title class=\"headline deep-orange\"> Warning</v-card-title>\n                <v-card-text>{{errorMsg}}</v-card-text>\n                <v-card-actions>\n                  <v-spacer></v-spacer>\n                  <v-btn color=\"deep-orange\" text @click=\"setErrorZero\">Cancel</v-btn>\n                </v-card-actions>\n              </v-card>\n            </v-dialog>\n          </v-row>\n        </template>\n        <template v-if=\"! indexData\">\n            <v-data-table item-key=\"name\" class=\"elevation-1\" loading loading-text=\"Loading... Please wait\"></v-data-table>\n        </template>\n        <v-data-table\n            :headers=\"headers\"\n            :items=\"indexData.datas\"\n            :search=\"search\"\n            sort-by=\"active\"\n            class=\"elevation-1\"\n        >\n            <template v-slot:top>\n            <v-toolbar flat color=\"\">\n                <v-toolbar-title class=\"font-weight-bold\">Question</v-toolbar-title>\n                <v-divider\n                class=\"mx-4\"\n                inset\n                vertical\n                ></v-divider>\n                <v-text-field v-model=\"search\" label=\"Search\" class=\"mx-4\"></v-text-field>\n                <v-dialog v-model=\"dialog\" max-width=\"500px\">\n                <template v-slot:activator=\"{ on, attrs }\">\n                    <v-btn\n                    color=\"primary\"\n                    dark\n                    class=\"mb-2\"\n                    v-bind=\"attrs\"\n                    v-on=\"on\"\n                    >New Question</v-btn>\n                </template>\n                <v-card>\n                    <v-card-title>\n                    <span class=\"headline\">{{ formTitle }}</span>\n                    </v-card-title>\n\n                    <v-card-text>\n                    <v-container>\n                        <v-row>\n                            <v-col cols=\"12\" sm=\"6\" md=\"4\">\n                                <v-select\n                                v-model=\"editedItem.lesson_id\"\n                                :items=\"indexData.lessons\"\n                                item-text=\"name\" \n                                item-value=\"id\"\n                                label=\"Select Lesson\"\n                                ></v-select>\n                            </v-col>\n                            <v-col cols=\"12\" sm=\"6\" md=\"4\">\n                                <v-text-field v-model=\"editedItem.question\" label=\"Question Name\"></v-text-field>\n                            </v-col>\n                            <v-col cols=\"12\" sm=\"6\" md=\"4\">\n                                <v-text-field v-model=\"editedItem.option1\" label=\"Option1\"></v-text-field>\n                            </v-col>\n                            <v-col cols=\"12\" sm=\"6\" md=\"4\">\n                                <v-text-field v-model=\"editedItem.option2\" label=\"Option2\"></v-text-field>\n                            </v-col>\n                            <v-col cols=\"12\" sm=\"6\" md=\"4\">\n                                <v-text-field v-model=\"editedItem.option3\" label=\"Option3\"></v-text-field>\n                            </v-col>\n                            <v-col cols=\"12\" sm=\"6\" md=\"4\">\n                                <v-text-field v-model=\"editedItem.option4\" label=\"Option4\"></v-text-field>\n                            </v-col>\n                            <v-col cols=\"12\" sm=\"6\" md=\"4\">\n                                <v-text-field v-model=\"editedItem.correct_option\" label=\"Correct Option\"></v-text-field>\n                            </v-col>\n\n                            <v-col cols=\"12\" sm=\"6\" md=\"4\">\n                                <v-select\n                                v-model=\"editedItem.active\"\n                                :items=\"items\"\n                                label=\"Active\"\n                                ></v-select>\n                            </v-col>\n                        </v-row>\n                    </v-container>\n                    </v-card-text>\n\n                    <v-card-actions>\n                    <v-spacer></v-spacer>\n                    <v-btn color=\"blue darken-1\" text @click=\"close\">Cancel</v-btn>\n                    <v-btn color=\"blue darken-1\" text @click=\"save\">Save</v-btn>\n                    </v-card-actions>\n                </v-card>\n                </v-dialog>\n            </v-toolbar>\n            </template>\n            <template v-slot:item.actions=\"{ item }\">\n            <v-icon\n                small\n                class=\"mr-2\"\n                @click=\"editItem(item)\"\n            >\n                mdi-pencil\n            </v-icon>\n            <v-icon\n                small\n                @click=\"deleteItem(item)\"\n            >\n                mdi-delete\n            </v-icon>\n            </template>\n            <template v-slot:no-data>\n            <v-btn color=\"primary\" @click=\"initialize\">Reset</v-btn>\n            </template>\n        </v-data-table>\n    </v-container>\n    <v-container v-else>\n        <template>\n            <div>\n                <v-alert\n                v-model=\"alert\"\n                dismissible\n                close-icon=\"mdi-delete\"\n                color=\"red\"\n                border=\"left\"\n                elevation=\"2\"\n                colored-border\n                \n                >\n                You are <strong>not Logged In</strong>, please log in first!.\n                </v-alert>\n\n                <div class=\"text-center\">\n                <v-btn\n                    v-if=\"!alert\"\n                    dark\n                    @click=\"alert = true\"\n                >\n                    Reset Alert\n                </v-btn>\n                </div>\n            </div>\n        </template>\n    </v-container>\n</template>\n\n<script>\nimport { mapGetters } from 'vuex'\n\n  export default {\n    data: () => ({\n      dialog: false,\n      apiUrl: 'question',\n      alert:true,\n      search: '',\n      headers: [\n        { text: 'Course Name', align: 'start', value: 'course_name'},\n        { text: 'Lesson Name', value: 'lesson_name'},\n        { text: 'Question Name', value: 'question'},\n        { text: 'Option1', value: 'option1'},\n        { text: 'Option2', value: 'option2'},\n        { text: 'Option3', value: 'option3'},\n        { text: 'Option4', value: 'option4'},\n        { text: 'Correct Option', value: 'correct_option'},\n        { text: 'Active', value: 'active' },\n        { text: 'Actions', value: 'actions', sortable: false },\n      ],\n      items: [0 , 1],\n      editedIndex: -1,\n      editedItem: {\n        lesson_id: '',\n        question: '',\n        option1: '',\n        option2: '',\n        option3: '',\n        option4: '',\n        correct_option: '',\n        active: 1,\n      },\n      defaultItem: {\n        lesson_id: '',\n        question: '',\n        option1: '',\n        option2: '',\n        option3: '',\n        option4: '',\n        correct_option: '',\n        active: 1,\n      },\n    }),\n\n    computed: {\n        ...mapGetters(['isLoggedIn', 'indexData', 'error', 'statusCode', 'errorMsg']),\n      formTitle () {\n        return this.editedIndex === -1 ? 'New Question' : 'Edit Question'\n      },\n    },\n\n    watch: {\n      dialog (val) {\n        val || this.close()\n      },\n    },\n\n    created () {\n      this.initialize()\n    },\n\n    methods: {\n      initialize () {\n        this.$store.dispatch('setIndexData', [this.apiUrl])\n      },\n\n      setErrorZero () {\n        this.$store.dispatch('setErrorZero')\n      },\n\n      editItem (item) {\n        this.editedIndex = this.indexData.datas.indexOf(item)\n        this.editedItem = Object.assign({}, item)\n        this.dialog = true\n      },\n\n      deleteItem (item) {\n        const index = this.indexData.datas.indexOf(item)\n        confirm('Are you sure you want to delete this item?') && this.indexData.datas.splice(index, 1)\n        this.$store.dispatch('delete', [this.apiUrl, item.id])\n      },\n\n      close () {\n        this.dialog = false\n        this.$nextTick(() => {\n          this.editedItem = Object.assign({}, this.defaultItem)\n          this.editedIndex = -1\n        })\n      },\n\n      save () {\n        if (this.editedIndex > -1) {\n          this.$store.dispatch('store', [this.apiUrl, this.editedItem])\n          Object.assign(this.indexData.datas[this.editedIndex], this.editedItem)\n        } else {\n          this.$store.dispatch('store', [this.apiUrl, this.editedItem])\n          this.indexData.datas.push(this.editedItem)\n        }\n        this.close()\n      },\n    },\n  }\n</script>"],"sourceRoot":"src/views/question"}]}